{
    "name": "website",
    "version": "1.0.0",
    "engines": {
        "node": "17.3.x"
    },
    "description": "Personal website for Skyler Wittman",
    "main": "index.js",
    "scripts": {
        "build:css": "tailwindcss build -i ./public/css/input.css -o ./public/css/output.css",
        "build:css:local": "tailwindcss build -i ./public/css/input.css -o ./public/css/output.css --watch",
        "dev": "nodemon ./index.js",
        "ui": "browser-sync start --config bs-config.js",
        "prod": "pm2 start ./index.js",
        "production": "pm2 start ./index.js",
        "start": "npm run build:css && npm run dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SkylerWittman/website.git"
    },
    "keywords": [],
    "author": "Skyler Wittman",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SkylerWittman/website/issues"
    },
    "homepage": "https://github.com/SkylerWittman/website#readme",
    "devDependencies": {
        "browser-sync": "^2.27.7",
        "nodemon": "^2.0.15",
        "npm-check-updates": "^12.0.5"
    },
    "dependencies": {
        "autoprefixer": "^10.4.0",
        "ejs": "^3.1.6",
        "express": "^4.17.2",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.34",
        "pm2": "^5.1.2",
        "pug": "^3.0.2",
        "tailwindcss": "^3.0.7"
    }
}
