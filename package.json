{
    "name": "website",
    "version": "1.0.1",
    "engines": {
        "node": "18.13.x"
    },
    "description": "Personal website for Skyler Wittman",
    "main": "index.js",
    "scripts": {
        "build:css": "tailwindcss build -i ./public/css/input.css -o ./public/css/output.css",
        "build:css:watch": "tailwindcss build -i ./public/css/input.css -o ./public/css/output.css --watch",
        "dev": "nodemon ./index.js",
        "ui": "npm run build:css && npm run dev && browser-sync start --config bs-config.js",
        "prod": "npm install && npm run build:css && npm run pm2-start",
        "production": "npm run prod",
        "pm2-start": "pm2 start ./index.js",
        "start": "npm run build:css && npm run dev"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SkylerWittman/website.git"
    },
    "keywords": [],
    "author": "Skyler Wittman",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/SkylerWittman/website/issues"
    },
    "homepage": "https://github.com/SkylerWittman/website#readme",
    "devDependencies": {
        "browser-sync": "^2.27.7",
        "nodemon": "^2.0.15",
        "npm-check-updates": "^16.6.2"
    },
    "dependencies": {
        "autoprefixer": "^10.4.2",
        "debug": "^4.3.3",
        "ejs": "^3.1.8",
        "express": "^4.17.2",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.34",
        "pm2": "^5.1.2",
        "postcss": "^8.4.5",
        "tailwindcss": "^3.2.4",
        "uuid": "^8.3.2"
    }
}
